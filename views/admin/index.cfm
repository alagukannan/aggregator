<cfoutput><h2>Currently Aggregated feeds</h2>#renderView(view='admin/menubar',cahce="true",cacheTimeout="30",cacheLastAccessTimeout="30")#<div id="content">	<p>List of available blogs in the aggregator.</p>	<div class="messageBox">		#getPlugin("messagebox").renderit()#	</div>	<table id="blogsTable" class="datatable">		<thead>		<tr>			<th>Website URL</th>			<th>Title</th>			<th>Date Last Build</th>			<th>Last Updated</th>			<th>Status</th>			<th>Options</th>		</tr>		</thead>		<tbody>		<cfloop query="prc.qBlogs">				<tr>				<td><a href="#Websiteurl#">#Websiteurl#</a></td>				<td><a href="#blogURL#" title="#title#">#Title#</a></td>				<td>#dateFormat(dateBuilt,'short')# #timeformat(dateBuilt,"medium")# </td>				<td>#dateFormat(dateSumitted,'short')# #timeformat(dateSumitted,"medium")#</td>				<td>#iif(isActive,de('Active'),de('inActive'))#</td>				<td>				<ul>					<li><a href="#event.BuildLink('#event.getCurrentModule()#.admin.detail/pk/#id#')#" class="info">View</a></li>					<li><a href="#event.BuildLink(linkto='#event.getCurrentModule()#.admin.dodelete',queryString='pk=#id#')#" class="remove">Remove</a></li>				</ul>								</td>			</tr>		</cfloop>		</tbody>	</table>		<div class="clear">&nbsp;</div></div></cfoutput>