<cfoutput><h2>Add a new blogs feed</h2>#renderView(view='admin/menubar',cahce="true",cacheTimeout="30",cacheLastAccessTimeout="30")#<div id="content"><div class="messageBox">	#getPlugin("messagebox").renderit()#</div><form action="#event.BuildLink(linkto='#event.getCurrentModule()#.admin.doadd')#" method="post" id="addBlogform" class="form">		<p>Please provide the blogs RSS/Atom URL.</p>		<p>			<label for="feedURL">Blog Feed URL:</label>			<input minlength="10" maxlength="500" class="required url" name="feedURL" id="feedURL" size="60" />		</p>		<p>			<input type="submit" value="Process and Add Feed" class="submit">		</p></form></div></cfoutput>